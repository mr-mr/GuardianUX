// Create column classes
@for $i from 1 through $column-number {
	.desktop-#{$i}{
		position: relative;
		width: $column-width*$i + $column-gutter-width*($i - 1);
		float: left;
	}
}

.desktop-full {
	width: 100%;
}

.desktop-three-quarters {
	width: 100% * 3/4;
}

.desktop-half {
	width: 100% * 1/2;
